<Spinner>:
    color: 0.3, 0.6, 0.7, 1
    size_hint: 0.3, 0.3

<MyLayout>
    FloatLayout:
        size: root.height, root.width

        Spinner:
            id: menu_select
            text: "Select"
            values: root.spinner_vals
            size: 500, 50
            pos_hint: {'x' : 0.5, 'y' : 0.5, 'top' : 1}
